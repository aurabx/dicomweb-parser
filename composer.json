{
    "name": "aurabx/dicomweb-parser",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "aurabx/dicom-data": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.0",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Aurabx\\DicomWebParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aurabx\\DicomWebParser\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "php -d memory_limit=1G ./vendor/bin/phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
